{
    "api_key": "API密钥",
    "model_name": "deepseek-ai/DeepSeek-V3",
    "api_url": "https://api.siliconflow.cn/v1/chat/completions",
    "api_timeout": 60,
    "api_delay": 0.5,
    "max_tokens": 2048,
    "temperature": 0.7,
    "top_p": 0.7,
    "top_k": 50,
    "frequency_penalty": 0.5,
    "enable_thinking": false,
    "default_source_lang": "en",
    "default_target_lang": "zh",
    "default_file": "example_data.xlsx",
    "default_column": 2,
    "default_batch_size": 30,
    "max_batch_size": 100,
    "max_retries": 3,  
    "retry_delay": 1,   
    "translation_prompt": {
        "instruction": "将给定的多行JSON格式[待翻译的语言]列表翻译为[要翻译成的语言]",
        "requirements": [
            "保持专业术语准确性（人名/地名/技术名词等）",
            "语言自然流畅符合中文表达习惯",
            "严格保留原始JSON格式与结构",
            "仅输出翻译后的JSON内容",
            "禁止包含任何额外说明或字符"
        ],
        "output_format": "与输入完全一致的多行JSON格式中文列表"
    }
}
